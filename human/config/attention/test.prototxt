  bottom: "att_conv1"
  top: "att_conv1"
  name: "drop_att_conv1"
  type: "Dropout"
  dropout_param {
    dropout_ratio: 0.5
  }
}

layer {
  bottom: "att_conv1"
  top: "att_fc"
  name: "att_fc"
  type: "Convolution"
  convolution_param {
    num_output: 3
    kernel_size: 1
  }
}
layer {
  bottom: "att_fc"
  top: "attention"
  name: "attention"
  type: "Softmax"
}

############### collect the output from attention model ########
layer {
  bottom: "attention"
  top: "attention1"
  top: "attention2"
  top: "attention3"
  name: "slice_attention"
  type: "Slice"
  slice_param {
    axis: 1
    slice_point: 1
    slice_point: 2
  }
}

############### scale features #############
layer {
  bottom: "fc8_${EXP}"
  bottom: "attention1"
  top: "fc8_product"
  name: "fc8_product"
  type: "SpatialProduct"
}

layer {
  bottom: "fc8_${EXP}_res075_interp"
  bottom: "attention2"
  top: "fc8_res075_product" 
  name: "fc8_res075_product"
  type: "SpatialProduct"
}

layer {
  bottom: "fc8_${EXP}_res05_interp"
  bottom: "attention3"
  top: "fc8_res05_product" 
  name: "fc8_res05_product"
  type: "SpatialProduct"
}

### add features ###
layer {
  bottom: "fc8_product"
  bottom: "fc8_res075_product"
  bottom: "fc8_res05_product"
  top: "fc8_fusion"
  name: "fc8_fusion"
  type: "Eltwise"
  eltwise_param {
    operation: SUM
  }
}
