  name: "fc7_concat"
  type: "Concat"
  concat_param {
    axis: 1
  }
}

### attention model
layer {
  bottom: "fc7_concat"
  top: "att_conv1"
  name: "att_conv1"
  type: "Convolution"
  convolution_param {
    num_output: 512
    kernel_size: 3
    pad: 1
  }
}
layer {
  bottom: "att_conv1"
  top: "att_conv1"
  name: "relu_att_conv1"
  type: "ReLU"
}
